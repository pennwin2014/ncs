          </tr>
          <!-- [#SLPE#] -->
          <!-- [#SLPB#] -->
          <tr id=[#snum#]> 
            <td class="bord1" width="46">&nbsp; </td>
            <td class="bord1" width="227">&nbsp;</td>
            <td class="bord1" width="135">&nbsp; </td>
            <td class="bord1" width="146">&nbsp;</td>
            <td class="bord3" width="110">&nbsp;</td>
            <td class="bord3" width="185">&nbsp;</td>
          </tr>
          <!-- [#SLPE#] -->
        </table>
    
