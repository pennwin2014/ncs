  </tr>
<!-- [#SLPB#] -->
  <tr  onMouseOut="mouseouttr2(this)" onclick="showf('[#lid#]','[#lname#]','[#lvister#]','[#lflags#]','[#lwarnid#]')"> 
    <td width="20%"><img src="/images/joinbottom.gif" width="14" height="16"><input type="checkbox" style="width:18px;height:18px" name="ch[#num#]" value="[#lid#]" )"></td>
    <td width="80%" onClick="gotoRight('/pronline/Msg?FunName@ncsWebLim_DispWebPo&pid@[#lid#]&name@[#lname#]&vister@[#lvister#]','[#lwarnid#]')" ><a href="#xxm">[#lname#]</a></td>
  </tr>
