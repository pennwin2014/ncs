<script>

    function Add()
    {
	    if(form1.name.value=='')
	    {
	    	alert('请填写要添加的策略名称');
	    	return false;
	    	}
	    	return true;
    }
    function Mod()
    {
	    if(form1.name.value=='')
	    {
	    	alert('请先选择要修改的策略');
	    	return false;
	    	}
	    	return true;
    }
    function Del()
    {
    var flag=0;
    	 for(var i=0;i<document.all.DataTable.rows.length;i++)
	    {
	    if(document.all.DataTable.rows[i].cells[0].childNodes[1].checked)
	    {
		    flag=1;
		    break;
	    }
	    }
	    if(flag==0)
	    {
	    	alert('请先选择要删除的策略');
	    	return false;
	    }
	    	return true;
    }
  
    function showf(sid,name,vister,flags,warnid) {
        form1.sid.value=sid;
        form1.name.value=name;
        form1.vister.value=vister;
		    form1.flags.value=flags;
		    form1.warnid.value=warnid;

    } 
  
   function gotoRight(t){
       parent.sbpm_right.location.href=t
   }
 
   function goto(t){
       self.location.href=t
   }
      
</script>
